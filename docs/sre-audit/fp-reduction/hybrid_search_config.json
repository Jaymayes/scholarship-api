{
  "run_id": "CEOSPRINT-20260113-VERIFY-ZT3G-032",
  "config_version": "1.0.0",
  "hard_filters": ["deadline", "gpa", "residency", "major"],
  "deadline_buffer_days": 30,
  "gpa_tolerance": 0.1,
  "major_fuzzy_threshold": 0.8,
  "strictMajorMatch": true,
  "vector_weight": 0.7,
  "keyword_weight": 0.3,
  "filter_logic": {
    "deadline": {"enabled": true, "rule": "scholarship.deadline >= NOW()", "always_enforced": true},
    "gpa": {"enabled": true, "rule": "student.gpa >= scholarship.min_gpa - 0.1", "null_handling": "pass_if_null"},
    "residency": {"enabled": true, "rule": "student.state IN scholarship.residency_states OR states is empty"},
    "major": {"enabled": true, "rule": "student.major IN scholarship.fields_of_study OR fields is empty", "fuzzy_matching": true}
  },
  "endpoints": {
    "public_search": "/api/v1/search/hybrid/public",
    "authenticated_search": "/api/v1/search/hybrid"
  }
}
