  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Set timestamp for request tracking",
          "pm.globals.set('request_timestamp', new Date().toISOString());"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Basic response validation",
          "pm.test('Response time is less than 5000ms', function () {",
          "    pm.expect(pm.response.responseTime).to.be.below(5000);",
          "});",
          "",
          "pm.test('Status code is success (2xx)', function () {",
          "    pm.expect(pm.response.code).to.be.oneOf([200, 201, 202]);",
          "});",
          "",
          "// Log response time and status for performance tracking",
          "console.log('Response Time: ' + pm.response.responseTime + 'ms');",
          "console.log('Status: ' + pm.response.status);",
          "",
          "// Check for trace ID in response headers",
          "pm.test('Response has trace ID', function () {",
          "    pm.expect(pm.response.headers.has('X-Request-ID')).to.be.true;",
          "});"
        ]
      }
    }
  ]
